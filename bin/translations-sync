#!/usr/bin/env node
"use strict";
var sync = require('../lib/sync');
var program = require('commander');

program
  .version(require('../package.json').version)
  .usage('<master> <target>', 'syncs <master> locale with <target> locale')
  .parse(process.argv);

try {
  sync.apply(sync, program.args);
} catch(e) {
  console.log(e.message);
  process.exit(1);
}
